{
    "name": "bulletproof-nodejs",
    "version": "1.0.0",
    "description": "Bulletproof node.js",
    "main": "src/app.ts",
    "scripts": {
        "build": "tsc",
        "heroku-postbuild": "npm run build",
        "start": "nodemon",
        "inspect": "nodemon --inspect src/app.ts",
        "test": "mocha -r ts-node/register **/*.test.ts",
        "lint": "npm run lint:js ",
        "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
        "lint:js": "npm run lint:eslint src/",
        "lint:fix": "eslint --ignore-path .gitignore --ext .ts --fix src/",
        "pm2:start": "pm2 start npm --name laprosos -- start",
        "pm2:logs": "pm2 logs laprosos",
        "pm2:list": "pm2 list",
        "pm2:show": "pm2 show laprosos",
        "pm2:stop": "pm2 stop laprosos",
        "pm2:restart": "pm2 restart laprosos --update-env",
        "pm2:delete": "pm2 delete laprosos"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/santiq/bulletproof-nodejs.git"
    },
    "rules": {
        "specific-rule": "off"
    },
    "keywords": [
        "boilerplay",
        "cron",
        "jobs",
        "js",
        "javascript",
        "typescript",
        "node",
        "express"
    ],
    "author": "Santiago Quinteros",
    "license": "ISC",
    "dependencies": {
        "@types/three": "^0.158.1",
        "acorn": "^7.1.0",
        "acorn-jsx": "^5.1.0",
        "agenda": "^5.0.0",
        "agendash": "^4.0.0",
        "argon2": "^0.26.0",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.2",
        "celebrate": "^9.1.0",
        "chai": "^4.3.10",
        "cors": "^2.8.4",
        "dotenv": "^5.0.1",
        "errorhandler": "^1.5.0",
        "event-dispatch": "^0.4.1",
        "eventemitter3": "^3.1.0",
        "express": "^4.18.2",
        "express-basic-auth": "^1.2.0",
        "express-jwt": "^8.4.1",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "method-override": "^3.0.0",
        "moment": "^2.23.0",
        "moment-timezone": "^0.5.23",
        "mongodb": "^4.2.1",
        "mongoose": "^6.12.1",
        "morgan": "^1.9.1",
        "pm2": "^5.3.0",
        "reflect-metadata": "^0.1.13",
        "shallow-equal-object": "^1.1.1",
        "sinon": "^7.5.0",
        "sinon-chai": "^3.3.0",
        "three": "^0.158.0",
        "typedi": "^0.10.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/agenda": "^4.1.0",
        "@types/chai": "^4.3.9",
        "@types/express": "^4.17.20",
        "@types/jest": "^23.3.14",
        "@types/lodash": "^4.14.118",
        "@types/mocha": "^10.0.0",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^16.11.12",
        "@typescript-eslint/eslint-plugin": "^6.7.3",
        "@typescript-eslint/parser": "^6.7.3",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^4.2.0",
        "eslint-plugin-prettier": "^3.0.1",
        "jest": "^29.7.0",
        "mocha": "^10.1.0",
        "prettier": "^1.17.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.3",
        "typescript": "^4.5.2"
    },
    "bugs": {
        "url": "https://github.com/santiq/bulletproof-nodejs/issues"
    },
    "homepage": "https://github.com/santiq/bulletproof-nodejs#readme"
}
